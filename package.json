{
  "name": "dest-toolkit",
  "version": "0.1.18",
  "private": true,
  "description": "A testing framework for database",
  "keywords": [
    "test",
    "database"
  ],
  "homepage": "https://github.com/peidongxie/dest#readme",
  "bugs": {
    "url": "https://github.com/peidongxie/dest/issues",
    "email": "peidongxie@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peidongxie/dest.git"
  },
  "license": "MIT",
  "author": {
    "name": "谢沛东",
    "email": "peidongxie@outlook.com",
    "url": "https://github.com/peidongxie"
  },
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:lib": "pnpm --filter @dest-toolkit/grpc-* --filter @dest-toolkit/http-* build",
    "prepare": "husky install",
    "publish:lib": "pnpm -r publish",
    "stats": "cloc $(git ls-files) && echo 'Commits:' $(git log --oneline | wc -l)"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "sort-package-json": "^2.1.0",
    "typescript": "^4.9.4"
  },
  "packageManager": "pnpm@7.2.1"
}
